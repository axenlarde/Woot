<!--Fichier de paramétrage de la collecte-->
<!--
Rappel :
Nom/Description du site = desc
Surnom du site = office.name
Pays = country
sda = sda
E164 prefixe = E164
Num standard = standard
Tranche SDA = tranche

Texte libre à mettre entre guillement simple, exemple = 'test'
utiliser des + pour séparer les éléments
-->
<xml>
	<template>
		<misc>
			<name>template1</name>
		</misc>
		<location>
			<action>inject</action>
			<name>'L_'+office.name</name>
			<audiobandwidth>office.audiobandwidth</audiobandwidth>
			<videobandwidth>office.videobandwidth</videobandwidth>
		</location>
	</template>
	<template>
		<misc>
			<name>template2</name>
		</misc>
		<location>
			<action>inject</action>
			<name>'L_'+office.name</name>
			<audiobandwidth>office.audiobandwidth</audiobandwidth>
			<videobandwidth>office.videobandwidth</videobandwidth>
		</location>
		<region>
			<action>inject</action>
			<name>'R_'+office.name</name>
			<defaultcodec>''</defaultcodec>
			<videobandwidth>office.videobandwidth</videobandwidth>
			<codecpreferencelist></codecpreferencelist>
			<g711>'R_'+office.name</g711>
			<g711>'CERTI_G711_Servers'</g711>
			<g711>'VCS'</g711>
		</region>
	</template>
</xml>